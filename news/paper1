    (function () {
        (new FontFaceObserver('Montserrat', {weight: 700})).load().then(function () {
            document.documentElement.className += ' font-loaded-montserrat-700';
        });
        (new FontFaceObserver('Montserrat', {weight: 400})).load().then(function () {
            document.documentElement.className += ' font-loaded-montserrat-400';
        });
    })();
